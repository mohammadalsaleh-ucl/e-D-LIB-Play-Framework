# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page

GET     /                       controllers.LoginController.main(request: Request)

+nocsrf
POST     /validate               controllers.LoginController.validateLoginGet(request: Request)


GET     /users                   controllers.LoginController.users()



GET     /registerform             controllers.RegisterController.register(request: Request)


+nocsrf
POST     /addusers                controllers.RegisterController.registerUser(request: Request)

#POST    /addUserSubmit           controllers.RegisterController.addUserSubmit()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
