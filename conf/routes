# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page

GET     /                       controllers.LoginController.viewLogin(request: Request)

+nocsrf
POST     /validate               controllers.LoginController.userLogin(request: Request)


GET     /users                   controllers.UsersController.users()



GET     /registerform             controllers.RegisterController.viewRregister(request: Request)


+nocsrf
POST     /addusers                controllers.RegisterController.userRegister(request: Request)



GET     /projectform             controllers.ProjectController.viewProject(request: Request)

+nocsrf
POST     /addprojects                controllers.ProjectController.projectRegister(request: Request)
#POST    /addUserSubmit           controllers.RegisterController.addUserSubmit()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
